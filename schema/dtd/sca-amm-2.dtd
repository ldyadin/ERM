	seq 	       CDATA 		#REQUIRED
	confltr 	  CDATA 		#IMPLIED
	varnbr 	    CDATA 		#IMPLIED
	alunqi 	    CDATA 		#IMPLIED
	pgblknbr 	  CDATA 		#REQUIRED
	confnbr 	  CDATA 		#IMPLIED
	%revatt;                                                         >

