<!-- ===========================================================-->
<!-- AMM TOP LEVEL STRUCTURE 					                            -->
<!-- ===========================================================-->

<!ELEMENT sca-amm-2 (((title, legalntc?, mfmatr, chapter+) | 
              increv | tr+)+,
              (revst|revend|cocst|cocend|docunit|hotlink))        >
<!ATTLIST sca-amm-2	
        spl     CDATA     #REQUIRED
	model   CDATA     #REQUIRED
	oidate  CDATA     #REQUIRED
	revdate CDATA     #REQUIRED
	tsn     CDATA     #REQUIRED
	cus     CDATA     #IMPLIED
	cusname CDATA     #IMPLIED
	docnbr  CDATA     #IMPLIED
	lang    CDATA     #REQUIRED
	chg     (N|R|U)   "N"
	efflen  CDATA     #IMPLIED
	efftype CDATA     #IMPLIED                                        >

<!ELEMENT increv (transltr?, (legalntc?, (chapter
			| section | subject | pgblk | task
			| subtask | sheet | graphic | trlist
			| intro | effxref | sblist | coclist
			| drindex | hazmtlst | zac | zoning
			| acclst | glossary)+)) 		                                     >

<!ELEMENT tr (trfmatr, (chapter | section+ |
	subject+ | pgblk+ | task+ | subtask+ | sheet+
	| graphic+ | intro | effxref | sblist
	| coclist | drindex | legalntc
	| hazmtlst | zac | zoning | acclst
	| glossary)) 						                                             >

<!ELEMENT trfmatr (title, trreason, (%text;)?)                      >
<!ATTLIST trfmatr
	trnbr   CDATA     #REQUIRED
	trdel   CDATA     #IMPLIED
	trrepl 	 CDATA     #IMPLIED
	issdate CDATA     #REQUIRED                                        >

<!ELEMENT trreason (#PCDATA)                                        >

<!-- ===========================================================-->
<!-- Legal Notice Information 					                           -->
<!-- ===========================================================-->

<!ELEMENT legalntc (exprtcl|cpyrght|proptary|geninfo)+            >

<!ATTLIST legalntc %revatt;                                       >

<!ELEMENT proptary (fullstmt, partstmt?)		                       >

<!ELEMENT fullstmt (title?, cpyrght?, para+)		                    >

<!ELEMENT partstmt (title?, para+)			                            >
<!ELEMENT exprtcl (para | unlist | numlist | note)+	              >

<!ELEMENT cpyrght (year*, holder*, geninfo)		                     >

<!ELEMENT year (#PCDATA)				                                     >

<!ELEMENT holder (#PCDATA)				                                   >

<!ELEMENT geninfo (para | unlist | numlist | note)+	              >

<!-- ===========================================================-->
<!-- AMM FRONT MATTER                                           -->
<!-- ===========================================================-->

<!ELEMENT mfmatr (transltr?, trlist?, intro, effxref,
			     sblist, coclist?, drindex?, hazmtlst?,
			     zac?, glossary?) 			                                  >

<!ELEMENT transltr (chgdesc*, title, (prclist1 | %text;))        >
<!ATTLIST transltr
	%revatt;                                                        >

<!ELEMENT trlist ((%text;)?, (trdata+ | isempty))	                >
<!ATTLIST trlist
	%revatt; 						                                               >

<!ELEMENT trdata (trnbr, trstatus, trloc+)		                     >

<!ELEMENT intro (chgdesc*, title, prclist1)	                      >
<!ATTLIST intro
	%revatt; 						                                               >

<!ELEMENT effxref (chgdesc*, title, (%text;)?, 
				effdata+)			                                               >
<!ATTLIST effxref
	%revatt; 						                                               >

<!ELEMENT effdata (cus?, modtype?, cec, linenbr?, ect?,
			venbr?, esnbr?, benbr?, msnbr, acn?,
			operator?, fslgnbr?, cac?, cusaid?) 	                        >

<!ELEMENT ect (#PCDATA)				                                      >

<!ELEMENT esnbr (#PCDATA)				                                   >

<!ELEMENT cusaid (#PCDATA)				                                    >

<!ELEMENT cac (#PCDATA)				                                       >

<!ELEMENT fslgnbr (#PCDATA)				                                  >

<!ELEMENT operator (#PCDATA)				                                >

<!ELEMENT sblist (chgdesc*, title, %text;, (sbdata+
				| isempty)) 			                                             >
<!ATTLIST sblist %revatt; 						                                 >

<!ELEMENT sbdata (sbnbr?, sbrev?, sbtitle?, sbnrmfr?,
			eonbr*, atanbr?, mdnbr*, cocnbr*, sc?,
			aclist*, ics, custeff?) 		                                    >

<!ELEMENT sbnrmfr (sbnbr, mfr?)			                               >

<!ELEMENT mfr (#PCDATA)				                                       >

<!ELEMENT sbrev (#PCDATA)				                                    >

<!ELEMENT aclist EMPTY				                                       >

<!ATTLIST aclist
	status 	CDATA 		#IMPLIED
	effrg 	CDATA 		#IMPLIED 		                                        >

<!ELEMENT custeff (cus, effrc)+			                               >

<!ELEMENT coclist (chgdesc*, title, %text;, (cocdata+
				| isempty)) 			                                             >
<!ATTLIST coclist
	%revatt; 						                                                >

<!ELEMENT cocdata (cocnbr?, cocrev?, eonbr*, atanbr?,
				mdnbr*,	custeff?, sc?, aclist*, 
				title?, (icdate |ics)) 		                                   >

<!ELEMENT cocrev (#PCDATA)				                                   >

<!ELEMENT icdate (#PCDATA)				                                   >

<!ELEMENT eonbr  (#PCDATA)				                                   >

<!ELEMENT drindex (chgdesc*, title, %text;, drdata+)	             >
<!ATTLIST drindex
	%revatt; 						                                               >

<!ELEMENT drdata (refext, drdesc, refint+)		                     >

<!ELEMENT cus     (#PCDATA)				                                 >

<!ELEMENT effrc   (#PCDATA)				                                 >

<!ELEMENT modtype (#PCDATA)				                                 >

<!ELEMENT cec     (#PCDATA)				                                 >

<!ELEMENT linenbr (#PCDATA)				                                 >

<!ELEMENT venbr   (#PCDATA)				                                 >

<!ELEMENT benbr   (#PCDATA)				                                 >

<!--ELEMENT sc    (#PCDATA) -->

<!ELEMENT sbnbr   (#PCDATA)                                      >
<!ELEMENT sbtitle (#PCDATA)                                      >

<!ELEMENT acn     (#PCDATA)                                      >

<!ELEMENT drdesc  (#PCDATA)                                      >

<!ELEMENT atanbr  (#PCDATA)                                      >

<!ELEMENT mdnbr   (#PCDATA)                                      >

<!ELEMENT cocnbr  (#PCDATA)                                      >

<!ELEMENT ics     (#PCDATA)                                      >
<!ATTLIST ics
	status CDATA     #IMPLIED                                      >

<!ELEMENT msnbr   (#PCDATA)                                      >

<!ELEMENT trnbr   (#PCDATA)                                      >
<!ELEMENT trstatus (#PCDATA)                                     >

<!ELEMENT trloc   (#PCDATA)                                      >

<!ELEMENT hazmtlst (title, pretopic*, hazmt+)                    >
<!ATTLIST hazmtlst
	%revatt;                                                        >

<!ELEMENT hazmt (title, warning)                                 >
<!ATTLIST hazmt
	id ID #REQUIRED                                                 >

<!-- ===========================================================-->
<!-- Aircraft Zoning and Access 				                           -->
<!-- ===========================================================-->

<!ELEMENT zac (chgdesc*, zoning?, acclst?, graphic*)              >
<!ATTLIST zac
	%revatt;                                                         >

<!-- Access description -->

<!ELEMENT acclst (chgdesc*, title?, (%text;)?, (access+
| isempty))                                                       >
<!ATTLIST acclst
	%revatt;                                                         >

<!ELEMENT access (effect?, pan, accdata+)                         >

<!ELEMENT accdata (effect?, zone, accein?, accessto?,
fastener?, dd?, openhour?, grphcref*)                              >

<!ELEMENT accein (%einent;)+                                       >

<!ELEMENT accessto (#PCDATA)                                       >

<!ELEMENT fastener (fsttype, fstqty)                               >
<!ATTLIST fastener
	hing %yesorno;    #IMPLIED                                        >

<!ELEMENT fsttype  (#PCDATA)                                        >

<!ELEMENT fstqty   (#PCDATA)                                        >

<!ELEMENT dd EMPTY                                                 >
<!ATTLIST dd
	width  CDATA          #IMPLIED
	length CDATA          #IMPLIED
	dia    CDATA          #IMPLIED
	unit  (ft|in|mm|cm|m) #IMPLIED                                     >

<!ELEMENT openhour     (#PCDATA)                                    >

<!-- zones description -->

<!ELEMENT zoning (chgdesc*, title?, (%text;)?, (majzone+ | isempty))>
<!ATTLIST zoning
	%revatt;                                                           >

<!ELEMENT majzone (effect?, zone, zondata+, subzone*)                >

<!ELEMENT subzone (effect?, zone, zondata+, speczone*)               >

<!ELEMENT speczone (effect?, zone, zondata+)                         >

<!ELEMENT zondata (effect?, zonedesc, side, bndfrom?,
				bndto?, grphcref*) 		                                          >

<!ELEMENT side EMPTY                                                 >
<!ATTLIST side
	hand (LH | RH | LR) #REQUIRED                                       >

<!ELEMENT zonedesc   (#PCDATA)                                        >

<!ELEMENT bndfrom    (boundary)+                                      >

<!ELEMENT bndto      (boundary)+                                      >

<!ELEMENT boundary   (#PCDATA| frame| rib | stnnbr | stgr)*           >

<!ELEMENT frame      (#PCDATA)                                        >

<!ELEMENT stnnbr     (#PCDATA)                                        >

<!ELEMENT rib        (#PCDATA)                                        >

<!ELEMENT stgr       (#PCDATA)                                        >

<!-- ===========================================================-->
<!-- Glossary                                                   -->
<!-- ===========================================================-->

<!ELEMENT glossary ((chgdesc*, title, (%text;)?,
				glosdata*) | %deleted;)                                     >
<!ATTLIST glossary
	oldkey CDATA #IMPLIED
	%revatt;                                                        >

<!ELEMENT glosdata (term, def)                                    >

<!ELEMENT term     (#PCDATA)                                      >

<!ELEMENT def      (#PCDATA)                                      >

<!-- =========================================================== -->
<!-- CHAPTER                                                     -->
<!-- =========================================================== -->

<!ELEMENT chapter ((effect?, chgdesc*, title, chapterfm*, toplist*, chglist*, 
                  eplist, chatoc, section+) | %deleted;)         >
<!ATTLIST chapter
	chappref   	CDATA 		#IMPLIED
	chapnbr 	  CDATA 		#REQUIRED
	%revatt;                                                          >

<!ELEMENT chapterfm      (#PCDATA)                                  >
<!ATTLIST chapterfm
	model    	  CDATA 		#REQUIRED
	docnbr 	     CDATA 	#REQUIRED
	scope    	  CDATA 		#REQUIRED
 rev      	  CDATA 		#REQUIRED
 revyear   	 CDATA 		#REQUIRED
 book     	  CDATA 		#REQUIRED
 bookname  	 CDATA 		#REQUIRED                                      >

<!ELEMENT toplist     (#PCDATA)                                      >
<!ELEMENT chglist     (%text;)                                       >
<!ELEMENT eplist      (#PCDATA|table)*                               >
<!ELEMENT chatoc      (#PCDATA|table)*                               >

<!-- =========================================================== -->
<!-- SECTION                                                     -->
<!-- =========================================================== -->

<!ELEMENT section ((effect?, chgdesc*, title, subject+)
				| %deleted;)                                                 >
<!ATTLIST section
	chappref 	CDATA 		#IMPLIED
	chapnbr 	  CDATA 		#REQUIRED
	sectnbr 	  CDATA 		#REQUIRED
	%revatt;                                                          >

<!-- =========================================================== -->
<!-- SUBJECT                                                     -->
<!-- =========================================================== -->

<!ELEMENT subject ((effect?, chgdesc*, title, pgblk+)
