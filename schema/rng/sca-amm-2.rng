<grammar xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0" xmlns="http://relaxng.org/ns/structure/1.0" datatypeLibrary="http://www.w3.org/2001/XMLSchema-datatypes">
  <define name="g.r">
    <choice>
      <zeroOrMore>
        <ref name="refblock.element"/>
      </zeroOrMore>
      <group>
        <zeroOrMore>
          <ref name="grphcref.element"/>
        </zeroOrMore>
        <zeroOrMore>
          <ref name="refext.element"/>
        </zeroOrMore>
        <zeroOrMore>
          <ref name="refint.element"/>
        </zeroOrMore>
      </group>
    </choice>
  </define>
  <define name="w.c">
    <zeroOrMore>
      <ref name="warning.element"/>
    </zeroOrMore>
    <zeroOrMore>
      <ref name="caution.element"/>
    </zeroOrMore>
  </define>
  <define name="text">
    <oneOrMore>
      <choice>
        <ref name="para.element"/>
        <ref name="table.element"/>
        <ref name="unlist.element"/>
        <ref name="numlist.element"/>
        <ref name="note.element"/>
      </choice>
    </oneOrMore>
  </define>
  <define name="text_wo_note">
    <oneOrMore>
      <choice>
        <ref name="para.element"/>
        <ref name="table.element"/>
        <ref name="unlist.element"/>
        <ref name="numlist.element"/>
      </choice>
    </oneOrMore>
  </define>
  <define name="deleted">
    <ref name="deleted.element"/>
    <zeroOrMore>
      <ref name="chgdesc.element"/>
    </zeroOrMore>
  </define>
  <define name="yesorno">
    <data type="string" datatypeLibrary=""/>
  </define>
  <define name="revatt">
    <attribute name="chg">
      <choice>
        <value>N</value>
        <value>R</value>
        <value>U</value>
        <value>D</value>
      </choice>
    </attribute>
    <attribute name="key">
      <data type="ID"/>
    </attribute>
    <attribute name="revdate"/>
  </define>
  <define name="einent">
    <choice>
      <ref name="ein.element"/>
      <ref name="einmfr.element"/>
    </choice>
  </define>
