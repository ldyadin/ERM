  <!--  =========================================================== -->
  <!-- AMM TOP LEVEL STRUCTURE -->
  <!--  =========================================================== -->
  <define name="sca-amm-2.element">
    <element name="sca-amm-2">
      <ref name="sca-amm-2.attlist"/>
      <oneOrMore>
        <choice>
          <group>
            <ref name="title.element"/>
            <optional>
              <ref name="legalntc.element"/>
            </optional>
            <ref name="mfmatr.element"/>
            <oneOrMore>
              <ref name="chapter.element"/>
            </oneOrMore>
          </group>
          <ref name="increv.element"/>
          <oneOrMore>
            <ref name="tr.element"/>
          </oneOrMore>
        </choice>
      </oneOrMore>
      <choice>
        <ref name="revst.element"/>
        <ref name="revend.element"/>
        <ref name="cocst.element"/>
        <ref name="cocend.element"/>
        <ref name="docunit.element"/>
        <ref name="hotlink.element"/>
      </choice>
    </element>
  </define>
  <define name="sca-amm-2.attlist" combine="interleave">
    <attribute name="spl"/>
    <attribute name="model"/>
    <attribute name="oidate"/>
    <attribute name="revdate"/>
    <attribute name="tsn"/>
    <optional>
      <attribute name="cus"/>
    </optional>
    <optional>
      <attribute name="cusname"/>
    </optional>
    <optional>
      <attribute name="docnbr"/>
    </optional>
    <attribute name="lang"/>
    <optional>
      <attribute name="chg" a:defaultValue="N">
        <choice>
          <value>N</value>
          <value>R</value>
          <value>U</value>
        </choice>
      </attribute>
    </optional>
    <optional>
      <attribute name="efflen"/>
    </optional>
    <optional>
      <attribute name="efftype"/>
    </optional>
  </define>
  <define name="increv.element">
    <element name="increv">
      <ref name="increv.attlist"/>
      <optional>
        <ref name="transltr.element"/>
      </optional>
      <group>
        <optional>
          <ref name="legalntc.element"/>
        </optional>
        <oneOrMore>
          <choice>
            <ref name="chapter.element"/>
            <ref name="section.element"/>
            <ref name="subject.element"/>
            <ref name="pgblk.element"/>
            <ref name="task.element"/>
            <ref name="subtask.element"/>
            <ref name="sheet.element"/>
            <ref name="graphic.element"/>
            <ref name="trlist.element"/>
            <ref name="intro.element"/>
            <ref name="effxref.element"/>
            <ref name="sblist.element"/>
            <ref name="coclist.element"/>
            <ref name="drindex.element"/>
            <ref name="hazmtlst.element"/>
            <ref name="zac.element"/>
            <ref name="zoning.element"/>
            <ref name="acclst.element"/>
            <ref name="glossary.element"/>
          </choice>
        </oneOrMore>
      </group>
    </element>
  </define>
  <define name="increv.attlist" combine="interleave">
    <empty/>
  </define>
  <define name="tr.element">
    <element name="tr">
      <ref name="tr.attlist"/>
      <ref name="trfmatr.element"/>
      <choice>
        <ref name="chapter.element"/>
        <oneOrMore>
          <ref name="section.element"/>
        </oneOrMore>
        <oneOrMore>
          <ref name="subject.element"/>
        </oneOrMore>
        <oneOrMore>
          <ref name="pgblk.element"/>
        </oneOrMore>
        <oneOrMore>
          <ref name="task.element"/>
        </oneOrMore>
        <oneOrMore>
          <ref name="subtask.element"/>
